{
  "name": "angular2",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "config": "ts-node ./scripts/set-env.ts",
    "start": "npm run config -- --environment=dev && ng serve",
    "build": "npm run config -- --environment=dev && ng build --output-hashing=none --aot=false --build-optimizer=false && rsync -K -a dist/. ../public",
    "dev": "npm run config -- --environment=dev && NODE_ENV=\"development\" && cd angular && ng serve",
    "prod": "npm run config -- --environment=prod && ng build --prod --output-hashing=none --aot=false --build-optimizer=false && rsync -K -a dist/. ../public",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.0.0",
    "@angular/cdk": "^5.2.4",
    "@angular/common": "^5.0.0",
    "@angular/compiler": "^5.0.0",
    "@angular/core": "^5.0.0",
    "@angular/forms": "^5.0.0",
    "@angular/http": "^5.0.0",
    "@angular/material": "^5.2.4",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "@angular/router": "^5.0.0",
    "@tinymce/tinymce-angular": "^1.0.5",
    "@types/slick-carousel": "^1.6.32",
    "angular2-image-upload": "^1.0.0-rc.0",
    "angular2-wizard": "^0.4.0",
    "bootstrap": "^3.4.1",
    "core-js": "^2.4.1",
    "fs-extra": "^5.0.0",
    "jquery": "^3.3.1",
    "ng2-modal": "0.0.25",
    "ngx-bootstrap": "^2.0.0-beta.9",
    "rxjs": "^5.5.2",
    "tinymce": "^4.5.3",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "^1.5.3",
    "@angular/compiler-cli": "^5.0.0",
    "@angular/language-service": "^5.0.0",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/jquery": "^3.3.0",
    "@types/node": "~6.0.60",
    "angular2-fontawesome": "^0.9.3",
    "codelyzer": "~3.2.0",
    "dotenv": "^5.0.1",
    "font-awesome": "^4.7.0",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lodash": "^4.17.5",
    "protractor": "~5.1.2",
    "slick-carousel": "^1.8.1",
    "ts-node": "~3.2.0",
    "tslint": "~5.7.0",
    "typescript": "~2.4.2",
    "yargs": "^11.0.0"
  }
}
